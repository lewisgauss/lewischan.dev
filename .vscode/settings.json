{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "css.validate": false,
  "scss.validate": false,
  "stylelint.enable": true,
  "stylelint.config": null,
  "[makefile]": {
    "editor.insertSpaces": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.workingDirectories": ["."]
}
