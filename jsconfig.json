{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "public"
  ],
  "compilerOptions": {
    "baseUrl": "src",
    "moduleResolution": "node",
    "module": "es6",
    "target": "es2021"
  },
  "typeAcquisition": {
    "exclude": [
      "got",
      "micromark-util-decode",
      "micromark-util-encode",
      "ordered-binary",
      "source-map-js",
      "source-map"
    ]
  }
}
